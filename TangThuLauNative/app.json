{
  "expo": {
    "name": "TangThuLau",
    "slug": "TangThuLau",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "tangthulau",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.vouucac.tangthulau",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "@react-native-google-signin/google-signin",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9fc5aad3-55ba-4531-9c06-1ab323dc27a2"
      },
      "GOOGLE_CLIENT_ID_FOR_ANDROID": "810010866433-kbqfb3kei54sabibo6lvsn6r1ifq0cip.apps.googleusercontent.com",
      "GOOGLE_CLIENT_ID_FOR_IOS": "810010866433-kbqfb3kei54sabibo6lvsn6r1ifq0cip.apps.googleusercontent.com",
      "GOOGLE_CLIENT_ID_FOR_WEB": "810010866433-mt493hv6rhq9bpgj3ashd92r6cblc8is.apps.googleusercontent.com",
      "API_BASE_URL": "http://localhost:5000",
      "EXPO_OS": "android"
    }
  }
}